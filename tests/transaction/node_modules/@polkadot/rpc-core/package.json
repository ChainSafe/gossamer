{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/api/issues",
  "contributors": [],
  "description": "A JavaScript wrapper for the Polkadot JsonRPC interface",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/api/tree/master/packages/rpc-core#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/rpc-core",
  "repository": {
    "directory": "packages/rpc-core",
    "type": "git",
    "url": "https://github.com/polkadot-js/api.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./detectPackage.cjs"
  ],
  "type": "module",
  "version": "7.11.1",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./bundle.cjs",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./detectOther.cjs",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./detectPackage.cjs",
      "default": "./detectPackage.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.cjs": "./packageInfo.cjs",
    "./packageInfo.js": "./packageInfo.js",
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "require": "./types/index.cjs",
      "default": "./types/index.js"
    },
    "./types/base": {
      "types": "./types/base.d.ts",
      "require": "./types/base.cjs",
      "default": "./types/base.js"
    },
    "./types/jsonrpc": {
      "types": "./types/jsonrpc.d.ts",
      "require": "./types/jsonrpc.cjs",
      "default": "./types/jsonrpc.js"
    },
    "./util": {
      "types": "./util/index.d.ts",
      "require": "./util/index.cjs",
      "default": "./util/index.js"
    },
    "./util/drr": {
      "types": "./util/drr.d.ts",
      "require": "./util/drr.cjs",
      "default": "./util/drr.js"
    },
    "./util/memo": {
      "types": "./util/memo.d.ts",
      "require": "./util/memo.cjs",
      "default": "./util/memo.js"
    },
    "./util/refCountDelay": {
      "types": "./util/refCountDelay.d.ts",
      "require": "./util/refCountDelay.cjs",
      "default": "./util/refCountDelay.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@polkadot/rpc-augment": "7.11.1",
    "@polkadot/rpc-provider": "7.11.1",
    "@polkadot/types": "7.11.1",
    "@polkadot/util": "^8.4.1",
    "rxjs": "^7.5.4"
  }
