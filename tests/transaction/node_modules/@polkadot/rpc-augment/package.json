{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/api/issues",
  "contributors": [],
  "description": "RPC generated augmentation",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/api/tree/master/packages/rpc-augment#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/rpc-augment",
  "repository": {
    "directory": "packages/rpc-augment",
    "type": "git",
    "url": "https://github.com/polkadot-js/api.git"
  },
  "sideEffects": [
    "./detectPackage.js",
    "./detectPackage.cjs"
  ],
  "type": "module",
  "version": "7.11.1",
  "main": "index.cjs",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./augment": {
      "types": "./augment/index.d.ts",
      "require": "./augment/index.cjs",
      "default": "./augment/index.js"
    },
    "./augment/jsonrpc": {
      "types": "./augment/jsonrpc.d.ts",
      "require": "./augment/jsonrpc.cjs",
      "default": "./augment/jsonrpc.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "require": "./bundle.cjs",
      "default": "./bundle.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./detectOther.cjs",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./detectPackage.cjs",
      "default": "./detectPackage.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.cjs": "./packageInfo.cjs",
    "./packageInfo.js": "./packageInfo.js",
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@polkadot/rpc-core": "7.11.1",
    "@polkadot/types": "7.11.1",
    "@polkadot/types-codec": "7.11.1",
    "@polkadot/util": "^8.4.1"
  }
}