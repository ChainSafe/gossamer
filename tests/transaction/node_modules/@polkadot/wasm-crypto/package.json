    "./detectPackage.js",
    "./detectPackage.cjs"
  ],
  "type": "module",
  "version": "4.5.1",
  "main": "index.js",
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@polkadot/wasm-crypto-asmjs": "^4.5.1",
