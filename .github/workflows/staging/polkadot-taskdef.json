{
    "containerDefinitions": [
        {
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "gossamer-staging-CWLogsGroup-wPH0S1xHYmq7",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "portMappings": [
                {
                    "containerPort": 7001,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8546,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 8540,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 6060,
                    "protocol": "tcp"
                }
            ],
            "environment": [],
            "mountPoints": [
                {
                    "sourceVolume": "gossamer-db-shared-disk",
                    "containerPath": "/root/.gossamer/westend/db"
                }
            ],
            "image": "500822580415.dkr.ecr.us-east-2.amazonaws.com/gossamer-polkadot",
            "essential": true,
            "name": "gossamer-polkadot"
        }
    ],
    "placementConstraints": [
        {
            "type": "memberOf",
            "expression": "attribute:ecs.availability-zone==us-east-2a"
        }
    ],
    "memory": "12288",
    "family": "gossamer-polkadot",
    "networkMode": "host",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "4096",
    "volumes": [
        {
            "name": "gossamer-db-shared-disk",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0568b7d3237cb1986",
                "rootDirectory": "/"
            }
        }
    ]
}